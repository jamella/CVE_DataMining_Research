        "CVE": [
            {
                "url": "http://cve.mitre.org/cgi-bin/cvename.cgi?name=CVE-2007-1995", 
                "published": "2007-04-12T06:19:00.000-04:00", 
                "id": "CVE-2007-1995", 
                "modified": "2011-03-31T00:00:00.000-04:00", 
                "summary": "bgpd/bgp_attr.c in Quagga 0.98.6 and earlier, and 0.99.6 and earlier 0.99 versions, does not validate length values in the MP_REACH_NLRI and MP_UNREACH_NLRI attributes, which allows remote attackers to cause a denial of service (daemon crash or exit) via crafted UPDATE messages that trigger an assertion error or out of bounds read."
            }
